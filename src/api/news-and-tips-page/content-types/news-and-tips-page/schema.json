{
  "kind": "singleType",
  "collectionName": "news_and_tips_pages",
  "info": {
    "singularName": "news-and-tips-page",
    "pluralName": "news-and-tips-pages",
    "displayName": "News & Tips Page",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "pluginOptions": {},
      "type": "string",
      "required": false
    },
    "colour": {
      "pluginOptions": {},
      "type": "string",
      "regex": "^#?([\\da-fA-F]{2})([\\da-fA-F]{2})([\\da-fA-F]{2})$",
      "required": true
    },
    "iconStyle": {
      "pluginOptions": {},
      "type": "enumeration",
      "enum": [
        "Solid",
        "Light",
        "Regular"
      ],
      "default": "Regular",
      "required": true
    },
    "icon": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "featuredArticles": {
      "type": "dynamiczone",
      "components": [
        "layout.article"
      ],
      "max": 4
    }
  }
}
