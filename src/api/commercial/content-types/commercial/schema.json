      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "address": {
      "type": "string"
    },
    "items": {
      "type": "richtext"
    },
